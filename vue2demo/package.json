{
  "name": "vue2demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "pnpm lint && vue-cli-service build",
    "build:client": "pnpm lint && VUE_CLI_MODE=client vue-cli-service build",
    "build:server": "pnpm lint && VUE_CLI_MODE=server vue-cli-service build",
    "build:ssr": "pnpm build:client && pnpm build:server",
    "build:servering": "node server.js",
    "lint": "eslint --fix"
  },
  "dependencies": {
    "core-js": "^3.30.2",
    "express": "^4.18.2",
    "vue": "^2.7.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5",
    "vue-server-renderer": "2.7.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "lint-staged": "^11.2.6",
    "sass": "^1.62.1",
    "sass-loader": "^12.6.0",
    "typescript": "~4.5.5",
    "vue-template-compiler": "^2.7.14"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
